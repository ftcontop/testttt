-- ========== LOGIC LOOPS ==========

-- PULL VECTOR LOOP (EXACT FROM FTCHUB)
_lOoo1lloOl.Heartbeat:Connect(function(dt)
    if not cfg.pullEnabled then 
        _lO0oOIll1o = nil
        return 
    end
    if not holdingM1 then 
        _lO0oOIll1o = nil
        return 
    end
    
    if not ballPart or not ballPart.Parent then
        _lO0oOIll1o = nil
        return
    end
    
    if _lO0oOIll1o then
        _O101lO = (ballPart.Position - _lO0oOIll1o) / dt
    end
    _lO0oOIll1o = ballPart.Position
    
    local _olO1lo1 = hrp.Position
    local _000I0l1ll = ballPart.Position
    local _olIolOloO = _O101lO.Magnitude
    
    local _00oO1o1II = cfg.pullSpeed * 2.0
    local _0110IIII1l = solveInterceptTime(_olO1lo1, _000I0l1ll, _O101lO, _00oO1o1II)
    
    if not _0110IIII1l then
        _0110IIII1l = 0.5
    end
    
    _0110IIII1l = math.clamp(_0110IIII1l, 0.1, 1.0)
    
    local _o0o1Ooolo = Vector3.new(0, -_O00lIlO.Gravity * 0.5, 0)
    local _Il0o1lll1 = _000I0l1ll + (_O101lO * _0110IIII1l) + (0.5 * _o0o1Ooolo * _0110IIII1l * _0110IIII1l)
    
    local _OI1lI111ol = (_Il0o1lll1 - _olO1lo1).Magnitude
    
    if _OI1lI111ol > cfg.pullDistance then return end
    if _OI1lI111ol <= cfg.pullOffset then return end
    
    local _0O1Illl11l = (_Il0o1lll1 - _olO1lo1).Unit
    
    local _OI1lIo1 = 2.0
    if _olIolOloO > 50 then
        _OI1lIo1 = 3.5
    elseif _olIolOloO > 30 then
        _OI1lIo1 = 2.8
    elseif _olIolOloO > 15 then
        _OI1lIo1 = 2.3
    end
    
    if _OI1lI111ol > cfg.pullDistance * 0.6 then
        _OI1lIo1 = _OI1lIo1 * 1.4
    end
    
    local _IlIOlllO = cfg.pullSpeed * _OI1lIo1 * dt
    local _00looolO = math.min(_IlIOlllO, _OI1lI111ol - cfg.pullOffset)
    
    local _OOI0IO0I11 = _olO1lo1 + (_0O1Illl11l * _00looolO)
    local _I1IoIIO = (_Il0o1lll1 - _OOI0IO0I11).Unit
    
    hrp.CFrame = CFrame.new(_OOI0IO0I11, _OOI0IO0I11 + _I1IoIIO)
end)

-- MAGNET LOOP (EXACT FROM FTCHUB)
_lOoo1lloOl.Heartbeat:Connect(function()
    if not cfg.magnetEnabled or not theonern or not theonern.Parent then 
        ifsm1gotfb = false
        prvnt = false
        return 
    end
    
    if not holdingM1 then
        prvnt = false
        return
    end
    
    ifsm1gotfb = ifsm1gotit()
    
    if ifsm1gotfb then
        prvnt = false
        return
    end
    
    local _l0loO0I, old = fbpos(theonern)
    local _Ol0110 = (hrp.Position - _l0loO0I).Magnitude
    if _Ol0110 > cfg.magnetDistance then 
        prvnt = false
        return 
    end
    
    local _0ol1OIOO0O = _l0loO0I - old
    local int1
    
    if _0ol1OIOO0O.Magnitude > 0.1 then
        int1 = _l0loO0I + (_0ol1OIOO0O.Unit * 12)
    else
        int1 = _l0loO0I + Vector3.new(5, 0, 5)
    end
    
    int1 = Vector3.new(int1.X, math.max(int1.Y, _l0loO0I.Y), int1.Z)
    
    og1 = hrp.CFrame
    prvnt = true
    hrp.CFrame = CFrame.new(int1)
    _lOoo1lloOl.RenderStepped:Wait()
    hrp.CFrame = og1
    prvnt = false
end)

-- AUTO TOUCHDOWN LOOP
local _O0o0OlIl = 0
local _1olo1IIo0 = 1

_lOoo1lloOl.Heartbeat:Connect(function()
    if not cfg.autofarmEnabled or not cfg.autoTouchdownEnabled then return end
    if not hasFootball() then return end
    
    if tick() - _O0o0OlIl < 2 then return end
    _O0o0OlIl = tick()
    
    if _1olo1IIo0 == 1 then
        hrp.CFrame = CFrame.new(cfg.touchdown1)
        _1olo1IIo0 = 2
    else
        hrp.CFrame = CFrame.new(cfg.touchdown2)
        _1olo1IIo0 = 1
    end
end)

-- Character respawn handler
_lIol1I.CharacterAdded:Connect(function(c2)
    char = c2
    hrp = c2:WaitForChild('HumanoidRootPartstring.char(41,10,32,32,32,32,104,117,109,97,110,111,105,100,32,61,32,99,50,58,87,97,105,116,70,111,114,67,104,105,108,100,40)Humanoid')
    prvnt = false
end)

-- Walkspeed (EXACT FROM FTCHUB)
task.spawn(function()
    while true do
        _lOoo1lloOl.Heartbeat:Wait()
        if cfg.walkspeedEnabled then
            local _o0111O0 = cfg.walkspeedBase or 16
            humanoid.WalkSpeed = _o0111O0 * cfg.walkspeedMultiplier
        end
    end
end)

-- Jump Power
local _O0OOl11lI = nil

task.spawn(function()
    while true do
        task.wait()
        
        if cfg.jumpPowerEnabled then
            if not _O0OOl11lI then
                _O0OOl11lI = humanoid.Jumping:Connect(function()
                    if hrp then
                        local _01IOo1Ol0 = Vector3.new(0, cfg.jumpPowerValue, 0)
                        hrp.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z) + _01IOo1Ol0
                    end
                end)
            end
        else
            if _O0OOl11lI then
                _O0OOl11lI:Disconnect()
                _O0OOl11lI = nil
            end
        end
    end
end)

-- ========== FINAL NOTIFICATION ==========
_110oooOOl:Notify({
    _OI1l1oIl = string.char(83,77,75,89,89,72,85,66),
    Content = string.char(83,99,114,105,112,116,32,108,111,97,100,101,100,32,115,117,99,99,101,115,115,102,117,108,108,121,33),
    Duration = 5
})


