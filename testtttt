    -- Make draggable
    local _1O1l0olOoI = false
    local _o0ooIO01l, dragStart, startPos
    
    local function update(input)
        local _l10oO0Il = input.Position - dragStart
        _I0O0I0.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + _l10oO0Il.X, startPos.Y.Scale, startPos.Y.Offset + _l10oO0Il.Y)
    end
    
    _I0O0I0.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            _1O1l0olOoI = true
            dragStart = input.Position
            startPos = _I0O0I0.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    _1O1l0olOoI = false
                end
            end)
        end
    end)
    
    _I0O0I0.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            _o0ooIO01l = input
        end
    end)
    
    game:GetService(string.char(85,115,101,114,73,110,112,117,116,83,101,114,118,105,99,101)).InputChanged:Connect(function(input)
        if input == _o0ooIO01l and _1O1l0olOoI then
            update(input)
        end
    end)
end

