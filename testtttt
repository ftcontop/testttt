-- ========== QB AIMBOT SYSTEM ==========
local _0101ll = {}
local _Il1olIO1l1 = {}
local _olIlI0 = 120
local _OOI0lOl = 80
local _111I1OI = workspace.Gravity or 196.2
local _0looOO = 3

local function getBallSpeed(_ooOooollI)
    local _l011o1lIOl = 40
    local _ooo1l1olO1 = 120
    return _l011o1lIOl + ((_ooOooollI / 100) * (_ooo1l1olO1 - _l011o1lIOl))
end

local function updatePlayerTrack(player, curPos, curVel)
    local _l1llll1 = _0101ll[player] or {lastPos=curPos, lastVel=curVel, history={}}
    local _1110oO1I = (curVel - _l1llll1.lastVel)
    
    table.insert(_l1llll1.history, 1, curVel)
    if #_l1llll1.history > 5 then table.remove(_l1llll1.history) end
    
    local _10OO00l = Vector3.new(0,0,0)
    for _, v in ipairs(_l1llll1.history) do 
        _10OO00l = _10OO00l + v 
    end
    _10OO00l = _10OO00l / #_l1llll1.history
    
    _l1llll1.lastPos = curPos
    _l1llll1.lastVel = curVel
    _l1llll1._1110oO1I = _1110oO1I
    _l1llll1._10OO00l = _10OO00l
    _0101ll[player] = _l1llll1
    
    return _l1llll1
end

local function updateReceiverHistory(_l0loO0I)
    table.insert(_Il1olIO1l1, _l0loO0I)
    if #_Il1olIO1l1 > 5 then
        table.remove(_Il1olIO1l1, 1)
    end
end

local function getRouteType(history)
    if #history < 4 then return string.char(115,116,114,101,97,107) end
    
    local _ll0OloO, p4 = history[#history-3], history[#history]
    local _O10oOO1O = (p4 - _ll0OloO).Unit
    
    if math.abs(_O10oOO1O.X) < 0.3 and _O10oOO1O.Z > 0.7 then
        return string.char(115,116,114,101,97,107)
    elseif _O10oOO1O.X > 0.7 and _O10oOO1O.Z > 0.5 then
        return string.char(99,111,114,110,101,114)
    elseif _O10oOO1O.X > 0.7 and math.abs(_O10oOO1O.Z) < 0.3 then
        return string.char(111,117,116)
    else
        return string.char(115,116,114,101,97,107)
    end
end

local function simulateLanding(_loll1l, throwTarget, _ooOooollI)
    local _001I1l = getBallSpeed(_ooOooollI)
    local _00IOO10I0 = (throwTarget - _loll1l).Unit
    local _0lOo10OOO = Vector3.new(throwTarget.X - _loll1l.X, 0, throwTarget.Z - _loll1l.Z).Magnitude
    
    local _O1oooIl0, landingPos = nil, nil
    
    for angle = math.rad(5), math.rad(85), math.rad(1) do
        local _l00olOO = _001I1l * math.cos(angle)
        local _lOo0lIo0 = _001I1l * math.sin(angle)
        local _Ol1l0Oo = _0lOo10OOO / _l00olOO
        local _oIlOOlloIl = _loll1l.Y + _lOo0lIo0 * _Ol1l0Oo - 0.5 * _111I1OI * _Ol1l0Oo^2
        
        if math.abs(_oIlOOlloIl - throwTarget.Y) < 5 then
            _O1oooIl0 = _Ol1l0Oo
            local _oI1olIO01 = _loll1l.X + _00IOO10I0.X * _l00olOO * _Ol1l0Oo
            local _OOIIoO0 = _loll1l.Z + _00IOO10I0.Z * _l00olOO * _Ol1l0Oo
            landingPos = Vector3.new(_oI1olIO01, _0looOO, _OOIIoO0)
            break
        end
    end
    
    return landingPos, _O1oooIl0
end

local function getNearestPlayer()
    local _o11lIoO1 = _lIol1I:GetMouse()
    local _Il100I = nil
    local _1OIlolI01 = math.huge

    for _, otherPlayer in ipairs(_IOO1l1oO:GetPlayers()) do
        if otherPlayer ~= _lIol1I and otherPlayer.Character then
            local _IoOolOl = otherPlayer.Character:FindFirstChild(string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116))
            if _IoOolOl then
                local _oO1O10lO, onScreen = _loO1o1I00O:WorldToViewportPoint(_IoOolOl.Position)
                if onScreen then
                    local _010lOl = Vector2.new(_o11lIoO1.X, _o11lIoO1.Y)
                    local _I0oOoOO = Vector2.new(_oO1O10lO.X, _oO1O10lO.Y)
                    local _OI1lI111ol = (_010lOl - _I0oOoOO).Magnitude
                    
                    if _OI1lI111ol < _1OIlolI01 then
                        _1OIlolI01 = _OI1lI111ol
                        _Il100I = otherPlayer
                    end
                end
            end
        end
    end

    return _Il100I
end

local function throwToReceiver()
