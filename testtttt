-- ========== PULL VECTOR SYSTEM (EXACT FROM FTCHUB) ==========
local _lO0oOIll1o = nil
local _O101lO = Vector3.new(0, 0, 0)

local function solveInterceptTime(_olO1lo1, _000I0l1ll, ballVel, playerSpeed)
    local _1o1oOOOO1I = ballVel
    local _1o10o1 = _000I0l1ll - _olO1lo1
    
    local _0IlIloI = _1o1oOOOO1I.X*_1o1oOOOO1I.X + _1o1oOOOO1I.Y*_1o1oOOOO1I.Y + _1o1oOOOO1I.Z*_1o1oOOOO1I.Z - playerSpeed*playerSpeed
    local _OoOO10ll = 2 * (_1o10o1.X*_1o1oOOOO1I.X + _1o10o1.Y*_1o1oOOOO1I.Y + _1o10o1.Z*_1o1oOOOO1I.Z)
    local _0l0ol0Ol0O = _1o10o1.X*_1o10o1.X + _1o10o1.Y*_1o10o1.Y + _1o10o1.Z*_1o10o1.Z
    
    local _Ol0o0Io0O = _OoOO10ll*_OoOO10ll - 4*_0IlIloI*_0l0ol0Ol0O
    
    if _Ol0o0Io0O < 0 then
        return nil
    end
    
    local _ooOI010I0 = (-_OoOO10ll + math.sqrt(_Ol0o0Io0O)) / (2*_0IlIloI)
    local _O1OI00I0I = (-_OoOO10ll - math.sqrt(_Ol0o0Io0O)) / (2*_0IlIloI)
    
    local _Ol1l0Oo = nil
    if _ooOI010I0 > 0 and _O1OI00I0I > 0 then
        _Ol1l0Oo = math.min(_ooOI010I0, _O1OI00I0I)
    elseif _ooOI010I0 > 0 then
        _Ol1l0Oo = _ooOI010I0
    elseif _O1OI00I0I > 0 then
        _Ol1l0Oo = _O1OI00I0I
    end
    
    return _Ol1l0Oo
end

-- ========== JUMP BOOST SYSTEM ==========
local _OlOI0Il = true

local function applyJumpBoost(rootPart)
    local _Il010lo0l = Instance.new(string.char(66,111,100,121,86,101,108,111,99,105,116,121))
    _Il010lo0l.Velocity = Vector3.new(0, cfg.BOOST_FORCE_Y, 0)
    _Il010lo0l.MaxForce = Vector3.new(0, math.huge, 0)
    _Il010lo0l.P = 5000
    _Il010lo0l.Parent = rootPart
    game:GetService(string.char(68,101,98,114,105,115)):AddItem(_Il010lo0l, 0.2)
end

local function setupJumpBoost(char)
    local _OIlloo1I11 = char:WaitForChild(string.char(72,117,109,97,110,111,105,100,82,111,111,116,80,97,114,116))
    
    ConnectionManager:Add(string.char(74,117,109,112,66,111,111,115,116,84,111,117,99,104), _OIlloo1I11.Touched:Connect(function(hit)
        if not cfg.jumpBoostEnabled or not _OlOI0Il then return end
        if _OIlloo1I11.Velocity.Y >= -2 then return end
        
        local _1I1oOlI0lI = hit:FindFirstAncestorWhichIsA(string.char(77,111,100,101,108))
        local _10oo0lo = _1I1oOlI0lI and _1I1oOlI0lI:FindFirstChild(string.char(72,117,109,97,110,111,105,100))
        
        if _1I1oOlI0lI and _1I1oOlI0lI ~= char and _10oo0lo then
            if cfg.jumpBoostTradeMode then
                _OlOI0Il = false
                applyJumpBoost(_OIlloo1I11)
                task.delay(cfg.BOOST_COOLDOWN, function()
                    _OlOI0Il = true
                end)
            else
                local _1111I111lI = getFootball()
                if _1111I111lI then
                    local _OI1lI111ol = (_1111I111lI.Position - _OIlloo1I11.Position).Magnitude
                    if _OI1lI111ol <= cfg.BALL_DETECTION_RADIUS then
                        _OlOI0Il = false
                        applyJumpBoost(_OIlloo1I11)
                        task.delay(cfg.BOOST_COOLDOWN, function()
                            _OlOI0Il = true
                        end)
                    end
                end
            end
        end
    end))
end

_lIol1I.CharacterAdded:Connect(function(char)
    if cfg.jumpBoostEnabled then
        setupJumpBoost(char)
    end
end)

-- ========== AUTOFARM TOUCHDOWN ==========
local function hasFootball()
    if not ballPart or not ballPart.Parent then return false end
    if ballPart:IsDescendantOf(char) then return true end
    local _01o110O = (hrp.Position - ballPart.Position).Magnitude
    return _01o110O < 8
end

-- ========== INPUT HANDLERS ==========
_0o1IO1lIO0.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    -- QB Aimbot: H to select, T to throw
    if input.KeyCode == Enum.KeyCode.H and cfg.qbAimbotEnabled then
        local _1I0oolOIO = getNearestPlayer()
        cfg.selectedPlayer = _1I0oolOIO
        if _1I0oolOIO then
            _110oooOOl:Notify({
                _OI1l1oIl = string.char(81,66,32,65,105,109,98,111,116),
                Content = string.char(83,101,108,101,99,116,101,100,58,32) .. _1I0oolOIO.Name,
                Duration = 3
            })
        end
    end
    
    if input.KeyCode == Enum.KeyCode.T and cfg.qbAimbotEnabled then
        throwToReceiver()
    end
end)

